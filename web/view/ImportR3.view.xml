<mvc:View
        controllerName="com.modekzWaybill.controller.ImportR3"
        xmlns:l="sap.ui.layout"
        xmlns:mvc="sap.ui.core.mvc"
        xmlns="sap.m">
    <l:VerticalLayout
            class="sapUiContentPadding"
            width="100%">
        <l:content>
            <Link
                    text="Завод - БЕ (Sap)"
                    press="importWerks"/>

            <Link
                    text="Вид топлива (Sap)"
                    press="importGasType"/>

            <Link
                    text="Датчики (Wialon)"
                    press="loadWlnVehicle"/>

            <Link
                    text="Мед карты водителей (Текстовый файл)"
                    press="uploadDriverMedCards"/>

            <Link
                    text="ТС подрядчика (Текстовый файл)"
                    press="uploadEquipment"/>
        </l:content>
    </l:VerticalLayout>
</mvc:View>